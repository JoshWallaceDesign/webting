    <script src="index.js" defer></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/1.20.0/TweenMax.min.js"></script>
    <style>
      #video-grid {
        display: flex;
        justify-content: center;
        flex-wrap: wrap;
      }
      video {
        height: 450px;
        width: 700px;

        border-radius: 25px;
        border: solid 2px #4040d0;
        object-fit: cover;
        padding: 8px;
      }
    </style>
  </head>
  <body>
    <div class="container-fluid1">
      <div class="main__left">
        <div class="main__videos">
          <div id="video-grid"></div>
        </div>
        <div class="main__controls">
          <li>
            <div class="main__controls__block">
              <div
                onclick="playStop()"
                class="main__controls__button main__video_button"
              >
                <i class="fas fa-video"></i>
              </div>
            </div>
          </li>
          <li>
            <div class="main__controls__block">
              <div
                onclick="muteUnmute()"
                class="main__controls__button main__mute_button"
              >
                <i class="fas fa-microphone"></i>
              </div>
            </div>
          </lii>
          <li>
            <div class="main__controls__block">
              <div class="main__controls__button1">
                <i class="fas fa-phone-alt fa-rotate-180"></i>
              </div>
            </div>
          </li>
        </div>
      </div>
      <div class="floating-chat">
        <i class="fa fa-comments" aria-hidden="true"></i>
        <div class="chat">
          <div class="header">
            <button>
              <i class="fa fa-times" aria-hidden="true"></i>
            </button>
          </div>
          <ul class="messages">
            <li class="other">
              Olalalalalalalalalalalalalala
